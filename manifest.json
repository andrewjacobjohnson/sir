{
  "manifest_version": 2,
  "name": "Economist Print Edition Reader",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": [
      "https://www.economist.com/printedition/*"
    ],
    "js": ["jquery-3.2.1.min.js", "content.js"]
  }
  ],
    "permissions": [
        "storage",
        "tabs"
    ]
}